{
  "compilerOptions": {
    "lib": ["es6", "dom"],
    "module": "ESNext",
    "target": "es5",
    "incremental": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "declaration": true,
    "strict": true,
    "noImplicitReturns": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
  },
}
